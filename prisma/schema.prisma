// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}
  
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}



model Alert {
  id         Int     @id @default(autoincrement())
  severity   String
  cameraId   String
  location   String
  message    String
  probability Float
  timestamp  String
}
